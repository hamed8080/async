        },
        {
          "content" : [
            {
              "inlineContent" : [
                {
                  "text" : "The code to send on disconnect. The default is the normal close code for cleanly disconnecting a webSocket.",
                  "type" : "text"
                }
              ],
              "type" : "paragraph"
            }
          ],
          "name" : "closeCode"
        }
